{
  "name": "react-isomorphic-app",
  "version": "0.0.0",
  "description": "A Basic React Isomorphic App",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhart/react-server-example"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      "envify"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "server",
    "server-side"
  ],
  "author": {
    "name": "Dmitri Rabinowitz",
    "email": "dmitrirabinowitz+code@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^2.9.0",
    "body-parser": "^1.10.2",
    "browser-sync": "^1.9.1",
    "browserify": "^7.0.3",
    "envify": "^3.2.0",
    "eventemitter": "^0.3.3",
    "express": "^4.11.1",
    "flux": "^2.0.1",
    "gulp": "^3.8.10",
    "gulp-nodemon": "^1.0.5",
    "gulp-sass": "^1.3.2",
    "gulp-shell": "^0.2.11",
    "gulp-watch": "^4.1.0",
    "jquery": "^2.1.3",
    "literalify": "^0.3.0",
    "moment": "^2.9.0",
    "mongoose": "^3.8.21",
    "mongoose-timestamp": "^0.3.0",
    "object-assign": "^2.0.0",
    "react": "^0.12.2",
    "react-router": "^0.11.6",
    "react-tools": "^0.12.2",
    "reactify": "^1.0.0",
    "watchify": "^2.2.1"
  },
  "scripts": {
    "start": "nodemon index.js",
    "watch": "watchify -d jsx/Router.js -o client/bundle.js -v",
    "build": "browserify jsx/Router.js -o client/bundle.js",
    "jsx-watch": "jsx --watch --no-cache-dir jsx/ src/",
    "jsx": "jsx --no-cache-dir jsx/ src/",
    "postinstall": "npm run jsx && npm run build"
  }
}
